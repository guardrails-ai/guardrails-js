{
  "name": "guardrails-ai",
  "version": "0.0.0",
  "description": "A Javascript wrapper for guardrails-ai",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src --quiet",
    "lint-fix": "eslint --fix ./src",
    "postinstall": "python3 -m pip install guardrails-ai",
    "prepack": "rm -rf ./dist; npm run build",
    "test": "jest ./tests",
    "test-cov": "jest ./tests --coverage",
    "test-file": "jest ./tests/guard.test.ts",
    "qa": "npm run lint; npm run test-cov;"
  },
  "author": "Guardrails AI",
  "license": "Apache-2.0",
  "repository": {
    "type": "github",
    "url": "https://github.com/guardrails-ai/guardrails-js"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "pythonia": "^1.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
